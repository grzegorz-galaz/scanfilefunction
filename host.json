{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      }
    },
    "logLevel": {
      "Function": "Information",
      "default": "Information"
    }
  },
  "extensions": {
    "eventGrid": {
      "maxEventsPerBatch": 1,
      "maxBatchSize": 1
    }
  }
}
